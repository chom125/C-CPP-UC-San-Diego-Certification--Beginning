 *
 *The function MyStrlen() calculates the length of
 *the string in s1 and returns the result.
 */
#include <stddef.h>

//This will calculate string length;
size_t MyStrlen(const char *s1)
{
    const char *s2;
    for (s2 = s1; *s2; ++s2)  // Loop to find end of string
        ;
    return (size_t)(s2 - s1);
}
